# Environment Configuration for Quiz Agent with A2A and MCP Support

# Server Configuration
PORT=3000

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Tavily Search API Configuration
TAVILY_API_KEY=your_tavily_api_key_here

# Protocol Configuration
PROTOCOLS_AUTO_INIT=true

# MCP (Model Context Protocol) Configuration
MCP_ENABLED=true
MCP_SERVER_ENABLED=true
MCP_SERVER_STDIO=true
MCP_CLIENT_ENABLED=true
MCP_AUTO_CONNECT=true

# A2A (Agent-to-Agent) Configuration
A2A_ENABLED=true
A2A_PORT=8080
A2A_AUTO_START=true
A2A_COORDINATOR_ENABLED=true

# External MCP Servers (optional)
# BRAVE_API_KEY=your_brave_search_api_key (for web search MCP server)

# Logging Configuration
LOG_LEVEL=info
DEBUG_PROTOCOLS=false

# Performance Configuration
DEFAULT_TIMEOUT=30000
MAX_CONCURRENT_TASKS=5
AGENT_RETRY_ATTEMPTS=3